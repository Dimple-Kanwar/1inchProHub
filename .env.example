
# Copy this file to .env and fill in your values

# 1inch API Configuration
VITE_ONEINCH_API_KEY=your_api_key_here
VITE_ONEINCH_BASE_URL=https://api.1inch.dev

# WebSocket Configuration
VITE_WS_URL=ws://localhost:5000

# Security Configuration
VITE_ENABLE_SECURITY_LOGGING=true
VITE_MAX_TRANSACTION_LIMIT=10000
VITE_DAILY_LIMIT_DEFAULT=5000
VITE_MONTHLY_LIMIT_DEFAULT=50000

# Development Settings
NODE_ENV=development
PORT=5000

# Database (if using)
DATABASE_URL=sqlite:./dev.db

# JWT Secret (for authentication)
JWT_SECRET=your_jwt_secret_here

# Additional API Keys
VITE_WALLET_CONNECT_PROJECT_ID=your_project_id_here
VITE_INFURA_API_KEY=your_infura_key_here
VITE_ALCHEMY_API_KEY=your_alchemy_key_here
